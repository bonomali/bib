scene.title={
 init=function()
	 anim=0
	 fx0=16 fy0=22 fx1=fx0+95 fy1=fy0+47
	 offt=75
	 code="111011110110211101120212201201221011021120101211012012102"
 end,
 update=function()
  if anim==0 then
   offt=lerp(offt,0,.2)
   if (flr(offt)==0) anim=1
  elseif anim==1 then
   anim=2
  elseif anim==2 then
   if (btnp(‚¨ÜÔ∏è) or btnp(‚¨áÔ∏è)) cur=not cur
   if btnp(üÖæÔ∏è) or btnp(‚ùé) then
    mode=cur and scene.game.story or scene.game.endless
    anim=3
   end
  elseif anim==3 then
   offt=lerp(offt,-75,.2)
   if (flr(offt)==-75) anim=4
  elseif anim==4 then
	  fx0=lerp(fx0,0,.15)
	  fy0=lerp(fy0,0,.15)
	  fx1=lerp(fx1,128,.15)
	  fy1=lerp(fy1,128,.15)
	  if flr(fx0)==flr(fy0) and flr(fx1)==flr(fy1) then
	   scene.active(cur and scene.op or scene.game)
	  end
  end
 end,
 draw=function()
  cls(7)
  -- header & footer
  morse(code,7,3)
  morse(code,7,124)
  -- select
  print("story",51,87,5)
  print("endless",51,96,5)
  sspr(85,3,9,9,35,cur and 85 or 94)
  pset(44,cur and 89 or 98,5)
  if cur then
   line(50,93,70,93,6)
  else
   line(50,102,78,102,6)
