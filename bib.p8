scene.op={
 init=function()
	 anim=0
	 skip=false
	 cx=0
	 ux=-18
	 rx=5 ry=26
	 px=23 py=-8 pfx=false
	 ey=124
 end,
 update=function()
  cx-=.025
  if anim==0 then
   ux=lerp(ux,17,.1)
   if (flr(ux)>=16) anim=1
  elseif anim==1 then
   ry=lerp(ry,104,.1) py=ry-8
   if (flr(ry)>=103) anim=2
  elseif anim==2 then
   rx-=1
   if (rx<=-1) anim=3
  elseif anim==3 then
   ux=lerp(ux,130,.1)
   if (cnt%10==0) pfx=not pfx
   if (flr(ux)==129) anim=4 cnt=0
  elseif anim==4 then
   if (cnt>=60) anim=5 cnt=0
  elseif anim==5 then
   if (ey>98) ey-=1
   if (cnt>=60) anim=6 cnt=0
  elseif anim==6 then
   if (cnt>=40) anim=7
  elseif anim==7 then
   px=lerp(px,100,.08)
   py=lerp(py,75,.08)
   if (flr(px)>=99 and flr(py)<=75) anim=8
  elseif anim==8 then
   anim=9
  elseif anim==9 then
   scene.active(scene.game)
  end
  if btnp(üÖæÔ∏è) or btnp(‚ùé) then
   if skip then
    anim=8
   else
    skip=true
   end
  end
 end,
 draw=function()
  cls(7)
  frame(0,0,127,127)
  -- cloud
  spr12(20,44+cx,8,2,1)
  spr12(20,90+cx,26,2,1)
  -- harajuku station
  sspr(0,77,44,51,46,37)
  sspr(49,123,29,5,40,62)
  sspr(8,77,29,51,90,37,29,51,true)
  sspr(44,86,5,42,119,45)
  if anim==1 or anim==2 then
   -- ray
	  line(27-rx,26,27-rx,ry,6)
	  line(28+rx,26,28+rx,ry,6)
	  line(28-rx,ry+1,27,ry+2)
	  line(27+rx,ry+1,28,ry+2)
  elseif anim==4 then
   -- balloon
   sspr(49,108,18,15,20,78+(cnt%20<10 and 0 or 1))
  elseif anim==5 or anim==6 then
   rect(33,99,36,101,5)
   -- envelope
   envelope(69,ey,ey<=98 and not(anim==6 and cnt>=30))
   if (anim==6) sspr(67,110,9,13,25,cnt<5 and 81 or 80)
  end
  -- text
  print("harajuku",7,7,5)
  line(6,13,38,13,6)
  print("üÖæÔ∏è/‚ùé to skip",70,116,skip and 6 or 7)
  -- player
  if (anim==7) clip(4,4,94,120)
  sspr(85,3,10,9,px,py,10,9,pfx)
  -- ufo
  spr12(17,ux,16)
  spr12(17,ux+12,16,1,1,true)
  if (anim==0 and cnt==0) or anim==8 or anim==9 then
   if (anim!=9) fillp(0x5a5a.8)
   rectfill(0,0,127,127,7)
   fillp()
  end
 end
