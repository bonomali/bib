-- bib=13113 (jis x 0402)

function _init()
 poke(0x5f15,133)
 poke(0x5f16,134)
	poke(0x5f5c,255) -- disable btnp repeat
 scene.active(scene.title)
end

scene={
 active=function(s)
 	cnt=0
  s.init()
  _update=function()
   cnt=(cnt+1)%120
   --printh(cnt)
   s.update()
  end
  _draw=s.draw
 end
}

scene.title={
 init=function()
	 anim=0
	 fx0=16 fy0=22 fx1=fx0+95 fy1=fy0+47
	 offt=75
	 code="111011110110211101120212201201221011021120101211012012102"
 end,
 update=function()
  if anim==0 then
   offt=lerp(offt,0,.2)
   if (flr(offt)==0) anim=1
  elseif anim==1 then
   anim=2
  elseif anim==2 and btnp(🅾️) then
   anim=3
  elseif anim==3 then
   offt=lerp(offt,-75,.2)
   if (flr(offt)==-75) anim=4
  elseif anim==4 then
	  fx0=lerp(fx0,0,.15)
	  fy0=lerp(fy0,0,.15)
	  fx1=lerp(fx1,128,.15)
	  fy1=lerp(fy1,128,.15)
	  if (flr(fx0)==flr(fy0) and flr(fx1)==flr(fy1)) scene.active(scene.game)
  end
 end,
 draw=function()
  cls(7)
  -- header & footer
  morse(code,7,3)
  morse(code,7,124)
  -- text
  spr12(1,22,82,1,2)
  print("press 🅾️ to start",39,93,cnt%40>=20 and 5 or 7)
  if anim<2 then
   if (anim==1) fillp(0x5a5a.8)
   rectfill(22,82,105,105,7)
   fillp()
  end
  -- title
  frame(fx0,fy0,fx1,fy1)
  if offt<75 and offt>-74 then
   local offx=fx0+offt+17
   local offy=fy0+10
   -- b1
	  sspr(96,0,8,28,offx,offy)
	  sspr(107,0,14,28,offx+8,offy)
	  -- i
	  sspr(96,0,11,28,offx+25,offy)
	  -- b2
	  sspr(96,0,8,28,offx+40,offy)
	  sspr(107,0,14,28,offx+48,offy)
  end
 end
}

scene.game={
 init=function()
  step=0
  speed=0
  ctime=0
  clouds={{x=124-flr(rnd(60)),y=4+flr(rnd(24))}}
  jump={flag=false,dy=-7,y=0}
  shot={flag=false,t=0}
  birds={}
  enemies={}
 end,
 update=function()
 	printh(step..","..cnt)
  -- clouds
  for c in all(clouds) do
   c.x-=.25
   if(c.x<=-20) del(clouds,c)
  end
  if(cnt==0 and rnd()<.6 and #clouds<2) add(clouds,{x=124,y=4+flr(rnd(24))})
  -- jump
  if((btnp(🅾️) or btn(⬆️)) and not shot.flag) jump.flag=true
  if(jump.flag) jump.y+=jump.dy jump.dy+=1
  if(jump.dy>7) jump.flag=false jump.dy=-7
  -- shot
  if((btnp(❎) or btn(⬇️)) and not jump.flag) shot.flag=true
  if(shot.flag) shot.t+=1
  if(shot.t>8) shot.flag=false shot.t=0
  -- birds
  for b in all(birds) do
   b.x-=3+speed
   if(b.x<=-8) del(birds,b)
   if iscollision(41,54+jump.y,8,7,b.x+1,b.y+1,10,4) then
    _draw()
    scene.active(scene.gameover)
   end
  end
  -- enemies
  for e in all(enemies) do
   e.x-=2+speed
   if shot.t>=4 and 64<e.x and e.x<87 then
    del(enemies,e)
    shot.flag=false
    shot.t=0
   end
   if e.x<=50 then
    _draw()
    scene.active(scene.gameover)
   end
  end
  if ctime>0 then
   ctime-=1
  elseif not((step==0 and cnt<60) or (step==14 and cnt>=60)) then
   ctime=10+flr(rnd(40))
  	if rnd()<.5 then
    add(birds,{x=124,y=50+flr(rnd(6))})
    ctime+=10
   else
    add(enemies,{x=124,y=65,stype=flr(rnd(7))})
   end
  end
 end,
 draw=function()
  cls(7)
  frame(0,0,127,127)
  print("🅾️/⬆️ to jump\n❎/⬇️ to shot",38,91,5)
  --clouds
  foreach(clouds, function(c) spr12(22,c.x,c.y,2,1) end)
  -- rail
  for i=0,10 do
	  spr12(27,4+i*12-(cnt*(1.5+speed))%4,70)
	 end
	 -- train
  spr12(40,4,56,4,2)
  -- birds
  foreach(birds, function(b) sspr(84,36+(flr(cnt/6)%2==0 and 0 or 6),12,6,b.x,b.y) end)
  -- enemies
  foreach(enemies, function(e) spr12(30+e.stype,e.x,e.y) end)
  -- player
  sspr(73,3,10,9,40,53+jump.y)
  -- shot
  if shot.flag then
   local ly=53+jump.y
   local dt=min(shot.t/3,1)
   line(45,ly,(81-45)*dt+45,(75-ly)*dt+ly,8)
  end
  -- target
  spr12t(26,6,5.5)
  -- map
  sspr(96,31,5,5,7,116)
  sspr(96,31,5,5,116,116)
  for i=0,12 do
  	sspr(i%2==0 and 101 or 109,32,8,3,12+8*i,117)
  end
  sspr(117,32,3,4,8+(step*120+cnt+1)/1800*109,111)
  -- step update
  if cnt==119 then
  	step+=1
  	speed=-1/(7^2)*(step-7)^2+1
  	if step>14 then
  	 printh("clear")
  	 scene.active(scene.gameover)
  	end
  end
 end
}

scene.gameover={
 init=function()
	 anim=0
	 poke(0x5f15,7)
	 poke(0x5f17,133)
 end,
 update=function()
  if anim==3 and btnp(🅾️) then
   scene.active(scene.game)
		 poke(0x5f15,133)
		 poke(0x5f17,7)
  end
 end,
 draw=function()
  if anim>=2 then
   local col=cnt%40<20 and 5 or 7
  	print("game over",46,91,col)
  	print("press 🅾️ to restart",26,97,col)
  end
  if anim==0 then
   fillp(0x5a5a.8)
  	rectfill(38,91,88,101,7)
   anim=1
  elseif anim==1 then
   fillp()
  	rectfill(38,91,88,101,7)
   anim=2
  elseif anim==2 then
   fillp(0x5a5a.8)
  	rectfill(26,91,100,101,7)
  	fillp()
  	anim=3
  end
 end
}
