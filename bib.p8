scene.game={
 story=0,endless=1,
 init=function()
  tx=-104
  score=0
  max_score=3*60 -- 60s
  speed=0
  ctime=0
  clouds={{x=124-flr(rnd(60)),y=4+flr(rnd(24))}}
  jump={flag=false,dy=-7,y=0}
  shot={flag=false,t=0}
  birds={}
  enemies={}
 end,
 update=function()
  -- score
 	if cnt%10==0 then
 	 score+=1
 	 if mode==scene.game.story then
 		 local spdx=score/max_score
  		speed=4*spdx*(1-spdx)
  		if score>max_score and tx>=124 then
  		 scene.active(scene.ed)
  		end
  	else
  	 speed=lerp(speed,2,.005)
  	end
 	end
 	-- train animation
  if (tx<-56) tx+=1.5
  if (mode==scene.game.story and score>=max_score) tx+=1.5
  -- clouds
  for c in all(clouds) do
   c.x-=.25
   if (c.x<=-20) del(clouds,c)
  end
  if (cnt==0 and rnd()<.6 and #clouds<2) add(clouds,{x=124,y=4+flr(rnd(24))})
  -- jump
  if (tx==-56 and (btnp(üÖæÔ∏è) or btn(‚¨ÜÔ∏è)) and not shot.flag) jump.flag=true
  if (jump.flag) jump.y+=jump.dy jump.dy+=1
  if (jump.dy>7) jump.flag=false jump.dy=-7
  -- shot
  if (tx==-56 and (btnp(‚ùé) or btn(‚¨áÔ∏è)) and not jump.flag) shot.flag=true
  if (shot.flag) shot.t+=1
  if (shot.t>8) shot.flag=false shot.t=0
  -- birds
  for b in all(birds) do
   b.x-=3+speed
   if (b.x<=-8) del(birds,b)
   if iscollision(41,54+jump.y,8,7,b.x+1,b.y+1,10,4) then
    _draw()
    scene.active(scene.gameover)
   end
  end
  -- enemies
  for e in all(enemies) do
   e.x-=2+speed
   if shot.t>=4 and 58<e.x and e.x<81 then
    del(enemies,e)
    shot.flag=false
    shot.t=0
   end
   if e.x<=50 then
    _draw()
    scene.active(scene.gameover)
   end
  end
  if ctime>0 then
   ctime-=1
  elseif not(score<6 or (mode==scene.game.story and score>=max_score-6)) then
   ctime=10+flr(rnd(40))
  	if rnd()<.5 then
    add(birds,{x=124,y=50+flr(rnd(6))})
    ctime+=10
   else
    add(enemies,{x=124,y=65,stype=flr(rnd(7))})
   end
  end
 end,
 draw=function()
  cls(7)
  frame(0,0,127,127)
  print("üÖæÔ∏è/‚¨ÜÔ∏è to jump\n‚ùé/‚¨áÔ∏è to shot",38,mode==scene.game.story and 91 or 97,5)
  --clouds
  foreach(clouds, function(c) spr12(20,c.x,c.y,2,1) end)
  -- rail
  for i=0,10 do
	  spr12(27,4+i*12-cnt*(tx==-56 and 1.5+speed or 1)%4,70)
	 end
	 -- train
  spr12(2,tx,56,4.5,2,true)
  spr12(2,tx+54,56,4.5,2)
  -- birds
  foreach(birds, function(b) sspr(84,36+(flr(cnt/6)%2==0 and 0 or 6),12,6,b.x,b.y) end)
  -- enemies
  foreach(enemies, function(e) spr12(30+e.stype,e.x,e.y) end)
  -- player
  sspr(85,3,10,9,tx+96,53+jump.y)
  -- shot
  if shot.flag then
   local ly=53+jump.y
   local dt=min(shot.t/3,1)
   line(45,ly,(75-45)*dt+45,(75-ly)*dt+ly,8)
  end
  -- target
  if (tx==-56) spr12t(26,5.5,5.5)
  if mode==scene.game.story then
	  -- map
	  sspr(96,31,5,5,7,116)
 	 sspr(96,31,5,5,116,116)
	  for i=0,12 do
	  	sspr(i%2==0 and 101 or 109,32,8,3,12+8*i,117)
	  end
	  sspr(117,32,3,4,8+min(score/max_score,1)*109,111)
  else
 	 -- score
	  print("score:"..pad(score,5),78,7,5)
  end
  if (score==0 and cnt==0) or (score>max_score and tx>=124) then
   fillp(0x5a5a.8)
   rectfill(0,0,127,127,7)
   fillp()
  end
 end
}
