function _init()
 -- set palette
 poke(0x5f15,133)
 poke(0x5f16,134)
 -- disable btnp repeat
	poke(0x5f5c,255)
 cur=true
 scene.active(scene.title)
end

scene={
 active=function(s)
 	cnt=0
  s.init()
  _update=function()
   cnt=(cnt+1)%120
   s.update()
  end
  _draw=s.draw
 end
}
